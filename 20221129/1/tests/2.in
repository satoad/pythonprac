class C(metaclass=dump):
    def __init__(self, val):
        self.val = val

    def add(self, other, another=None):
        return self.val + other + (another or self.val)

    def mul(self, other=None):
        return f'something * {other} = idk'

    def abs(self):
        return abs(self.val)
c = C(10)
print(c.mul(9), c.abs)
print(c.add(9,another=10))